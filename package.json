	},
	"devDependencies": {
		"@ctx-core/ctx-core-package-tools": "workspace:^8.0.10",
		"@ctx-core/dev-tools": "workspace:^6.0.10",
		"typescript": "^4.3.4"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
