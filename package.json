{
	"name": "@ctx-core/scroll",
	"version": "13.0.0",
	"description": "ctx-core scroll",
	"keywords": [
		"ctx-core",
		"scroll"
	],
	"homepage": "https://github.com/ctx-core/scroll#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/scroll/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/scroll/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run compile",
		"clean": "rm -rf dist",
		"compile": "swc src --out-dir dist --copy-files --source-maps --config-file .swcrc",
		"exec": "$@",
		"prepublishOnly": "npm run clean && npm run build"
	},
	"dependencies": {
		"@ctx-core/object": "workspace:^22.0.0",
		"@ctx-core/store": "workspace:^27.0.46",
		"svelte": "3.44.2"
	},
	"devDependencies": {
		"@ctx-core/ctx-core-package-tools": "workspace:^8.0.85",
		"@swc/cli": "^0.1.52",
		"@swc/core": "^1.2.118",
		"typescript": "next"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
